{
  "name": "@yeger/vue2-masonry-wall",
  "version": "1.1.1",
  "description": "Responsive masonry layout with SSR support and zero dependencies for Vue 2.",
  "license": "MIT",
  "repository": "github:DerYeger/vue2-masonry-wall",
  "homepage": "https://vue2-masonry-wall.yeger.eu",
  "bugs": {
    "url": "https://github.com/DerYeger/vue2-masonry-wall/issues"
  },
  "author": {
    "name": "Jan MÃ¼ller",
    "url": "https://github.com/DerYeger"
  },
  "keywords": [
    "vue",
    "plugin",
    "component",
    "masonry",
    "wall"
  ],
  "main": "dist/masonry-wall.ssr.js",
  "browser": "dist/masonry-wall.esm.js",
  "module": "dist/masonry-wall.esm.js",
  "unpkg": "dist/masonry-wall.min.js",
  "types": "masonry-wall.d.ts",
  "files": [
    "dist/*",
    "masonry-wall.d.ts",
    "src/**/*.vue"
  ],
  "scripts": {
    "demo:serve": "vue-cli-service serve demo/serve.ts",
    "demo:build": "vue-cli-service build demo/serve.ts --dest=demo-dist",
    "test": "jest",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "postbuild": "bundlesize",
    "lint": "eslint --ext \".js,.ts,.vue\"",
    "pretty": "prettier --write \"./**/*.{js,json,html,scss,ts,vue,yml}\"",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/eslint-parser": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-typescript": "7.15.0",
    "@rollup/plugin-alias": "3.1.5",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-node-resolve": "13.0.4",
    "@rollup/plugin-replace": "3.0.0",
    "@types/jest": "27.0.1",
    "@types/resize-observer-browser": "0.1.6",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "@vue/cli-plugin-babel": "4.5.13",
    "@vue/cli-plugin-typescript": "4.5.13",
    "@vue/cli-service": "4.5.13",
    "@vue/test-utils": "1.2.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "27.0.6",
    "bundlesize": "0.18.1",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-vue": "7.16.0",
    "flush-promises": "1.0.2",
    "jest": "27.0.6",
    "minimist": "1.2.5",
    "prettier": "2.3.2",
    "rollup": "2.56.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-vue": "5.1.9",
    "ts-jest": "27.0.5",
    "typescript": "4.3.5",
    "vue": "2.6.14",
    "vue-jest": "3.0.7",
    "vue-template-compiler": "2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "engines": {
    "node": ">=12"
  },
  "bundlesize": [
    {
      "path": "./dist/*.js",
      "maxSize": "5kb"
    }
  ],
  "publishConfig": {
    "access": "public"
  }
}
